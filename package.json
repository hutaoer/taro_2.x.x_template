{
  "name": "taro-app",
  "version": "1.0.0",
  "private": true,
  "description": "taro 2.2.6 项目模板",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:h5:mock": "cross-env NODE_ENV=development TARO_APP_API=mock yarn dev:h5",
    "dev:h5:dev": "cross-env NODE_ENV=development TARO_APP_API=dev yarn dev:h5",
    "dev:h5:fat": "cross-env NODE_ENV=development TARO_APP_API=fat yarn dev:h5",
    "dev:h5:pro": "cross-env NODE_ENV=development TARO_APP_API=pro yarn dev:h5",
    "dev:weapp:mock": "cross-env NODE_ENV=development TARO_APP_API=mock yarn dev:weapp",
    "dev:weapp:dev": "cross-env NODE_ENV=development TARO_APP_API=dev yarn dev:weapp",
    "dev:weapp:fat": "cross-env NODE_ENV=development TARO_APP_API=fat yarn dev:weapp",
    "dev:weapp:pro": "cross-env NODE_ENV=development TARO_APP_API=pro yarn dev:weapp",
    "build:h5:dev": "cross-env NODE_ENV=production TARO_APP_API=dev yarn build:h5",
    "build:h5:fat": "cross-env NODE_ENV=production TARO_APP_API=fat yarn build:h5",
    "build:h5:pro": "cross-env NODE_ENV=production TARO_APP_API=pro yarn build:h5",
    "build:weapp:dev": "cross-env NODE_ENV=production TARO_APP_API=dev yarn build:weapp",
    "build:weapp:fat": "cross-env NODE_ENV=production TARO_APP_API=fat yarn build:weapp",
    "build:weapp:pro": "cross-env NODE_ENV=production TARO_APP_API=pro yarn build:weapp"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "2.2.6",
    "@tarojs/components-qa": "2.2.6",
    "@tarojs/redux": "^2.2.10",
    "@tarojs/redux-h5": "^2.2.10",
    "@tarojs/router": "2.2.6",
    "@tarojs/taro": "2.2.6",
    "@tarojs/taro-alipay": "2.2.6",
    "@tarojs/taro-h5": "2.2.6",
    "@tarojs/taro-qq": "2.2.6",
    "@tarojs/taro-quickapp": "2.2.6",
    "@tarojs/taro-rn": "2.2.6",
    "@tarojs/taro-swan": "2.2.6",
    "@tarojs/taro-tt": "2.2.6",
    "@tarojs/taro-weapp": "2.2.6",
    "babel-runtime": "^6.26.0",
    "dva-core": "^2.0.3",
    "dva-loading": "^3.0.21",
    "nerv-devtools": "^1.5.5",
    "nervjs": "^1.5.5",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "0.11.1"
  },
  "devDependencies": {
    "@tarojs/cli": "2.2.6",
    "@tarojs/mini-runner": "2.2.6",
    "@tarojs/plugin-mock": "^0.0.6",
    "@tarojs/webpack-runner": "2.2.6",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-config-taro": "2.2.6",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "2.2.6",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.2.6",
    "stylelint-taro-rn": "2.2.6"
  }
}
